{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "method": [
            "GET",
            "POST",
            "DELETE",
            "PUT"
        ]
      }
    ]
  }
  